{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "logo/Lalals Logo Main.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Documentation",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-documentation/introduction",
              "api-documentation/sample-post-request",
              "api-documentation/sample-get-request"
            ]
          },
          {
            "group": "Conversion Endpoints",
            "pages": [
              "api-documentation/endpoint/MusicAI",
              "api-documentation/endpoint/cover",
              "api-documentation/endpoint/voicetovoice",
              "api-documentation/endpoint/TextToSpeech",
              "api-documentation/endpoint/extraction",
              "api-documentation/endpoint/stemsseparation",
              "api-documentation/endpoint/denoise"
            ]
          },
          {
            "group": "Helper Endpoints",
            "pages": [
              "api-documentation/endpoint/getById",
              "api-documentation/endpoint/searchVoices",
              "api-documentation/endpoint/getAllVoices"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "http://localhost:3000/api-documentation/introduction",          
          "icon": "book-open-cover"
        }
      ]
    }
  },
  "logo": {
    "light": "logo/Lalals Header.jpg",
    "dark": "logo/Lalals Header White.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Lalals",
        "href": "https://lalals.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "API Key",
      "href": "https://lalals.com/api"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}